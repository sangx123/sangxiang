<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  <LinearLayout
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:layout_alignParentTop="true"
  android:id="@+id/lltopblack_write"
  >
     <include
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          layout="@layout/black_white_list_title"
     />
  </LinearLayout>
    <LinearLayout
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:layout_alignParentBottom="true"
    android:id="@+id/llbottomblack_write"
  >
     <include
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          layout="@layout/timesetting_bottom"
     />
  </LinearLayout>
  <LinearLayout
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_above="@id/llbottomblack_write"
  android:layout_below="@id/lltopblack_write"
  android:background="@drawable/fjwr_main_bg"
  >
     <winway.mdr.chinaunicom.current.listview.MyListView
     android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:id="@+id/lvblack_white_listview"
     android:cacheColorHint="#00000000"
     android:divider="@drawable/line"
     />
  </LinearLayout>
</RelativeLayout>
